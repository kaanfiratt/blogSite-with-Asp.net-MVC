@model IEnumerable<Myblogg.Kategori>
@{
	Layout = null;
}

<div>
	<div class="col-md-12 col-md-offset-0 text-center fh5co-heading fh5co-heading-sidebar">
		<h2><span>Categories</span></h2>
	</div>
	<ul class="category">
		@foreach (var kat in Model)
		{
			<li><a href="/Kategori/Index/@kat.ID">@kat.KategoriAdi (@kat.Makale.Count)<i class="icon-check"></i></a></li>
		}

	</ul>
</div>


